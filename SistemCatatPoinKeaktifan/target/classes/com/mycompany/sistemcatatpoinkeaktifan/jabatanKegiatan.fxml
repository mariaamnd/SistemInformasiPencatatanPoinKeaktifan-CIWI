            <Label layoutX="12.0" layoutY="122.0" text="Tingkat" />
            <ComboBox fx:id="comboTingkat" layoutX="11.0" layoutY="147.0" onAction="#comboChanged" prefWidth="150.0" promptText="-pilihan-" />
