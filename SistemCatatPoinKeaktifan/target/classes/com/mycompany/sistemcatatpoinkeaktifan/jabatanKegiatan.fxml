                  <TableColumn fx:id="colJenis" prefWidth="156.0" text="Jenis Jabatan" />
                  <TableColumn fx:id="colKeterangan" minWidth="0.0" prefWidth="188.80007934570312" text="Keterangan" />
                  <TableColumn fx:id="colMasa" minWidth="0.0" prefWidth="118.4000244140625" text="Masa Jabatan" />
