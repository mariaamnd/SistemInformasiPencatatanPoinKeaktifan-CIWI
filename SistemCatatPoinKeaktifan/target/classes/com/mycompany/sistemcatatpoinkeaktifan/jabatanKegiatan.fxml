      </AnchorPane>
   </left>
   <center>
      <AnchorPane prefHeight="481.0" prefWidth="434.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="jabatanTableView" layoutX="-37.0" layoutY="29.0" prefHeight="399.0" prefWidth="774.0">
